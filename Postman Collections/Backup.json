				{
					"name": "GET /pet/{petId}",
					"item": [
						{
							"name": "Get pet by id",
							"event": [
								{
									"listen": "prerequest",
									"script": {
										"id": "7b80054a-1682-4c4f-b2e4-298816a7cb21",
										"exec": [
											"const postPetRequest = {",
											"    url: 'https://petstore.swagger.io/v2/pet/',",
											"    method: 'POST',",
											"    header: {",
											"        'Content-Type': 'application/json',",
											"    },",
											"    body: JSON.stringify({",
											"        \"id\": 102938475,",
											"        \"category\": {",
											"            \"id\": 0,",
											"            \"name\": \"string\"",
											"        },",
											"        \"name\": \"doggie\",",
											"        \"photoUrls\": [",
											"            \"string\"",
											"        ],",
											"        \"tags\": [",
											"            {",
											"            \"id\": 0,",
											"            \"name\": \"string\"",
											"            }",
											"        ],",
											"        \"status\": \"available\"",
											"    })",
											"};",
											"",
											"pm.sendRequest(postPetRequest, function (error, response) {",
											"    let responseText = response.text().replace(/([^\"\\w-])([-0-9.]{8,100})([^\"\\w-])/g, '$1\\\"$2\\\"$3');",
											"    let jsonData = JSON.parse(responseText);",
											"    let id = jsonData.id.replace('\"',''); ",
											"",
											"    console.log(\"Pet id is \" + id);",
											"",
											"    pm.environment.set(\"petId\", id);",
											"});"
										],
										"type": "text/javascript",
										"packages": {}
									}
								},
								{
									"listen": "test",
									"script": {
										"id": "f90f7319-bd5d-4108-b0da-4f5779efad0d",
